# Possible color values (wrapped in $fg[ and ]):
#   black  red
#   green  yellow
#   blue   magenta
#   cyan   white
#   bold
#
# Other possible values:
#   $reset_color

NEUTRAL_COLOR="$fg[blue]"

DEFAULT_COLOR="$fg[magenta]"
ALERT_COLOR="$fg[red]"

PATH_COLOR=$NEUTRAL_COLOR

BRANCH_COLOR=$DEFAULT_COLOR
BRANCH_DIRTY_COLOR=$ALERT_COLOR

SYMBOL_COLOR=$DEFAULT_COLOR

GREP_COLOR=$ALERT_COLOR

